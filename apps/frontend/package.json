		"next": "^15.4.5",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-hook-form": "^7.61.1",
		"react-hot-toast": "^2.5.2",
		"react-tabs": "^6.1.0",
		"react-tiny-popover": "^8.1.6",
		"tailwind-merge": "^2.6.0",
		"zod": "^3.25.76",
		"zod-form-data": "^2.0.7"
	},
	"devDependencies": {
		"@shared/biome-config": "workspace:*",
